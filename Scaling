import cv2
import numpy as np

path=r'https://s.yimg.com/ny/api/res/1.2/sGMcJfaFINp3dG9yk97d4Q--~A/YXBwaWQ9aGlnaGxhbmRlcjtzbT0xO3c9NzY4O2g9NTEy/https://media.zenfs.com/en/afp.com/5b20db790d3b4f3129ecaf000f0574a6'
img = cv2.imread(path)

res = cv2.resize(img,None,fx=2, fy=2, interpolation = cv2.INTER_CUBIC)

height, width = img.shape[:2]
res = cv2.resize(img,(2*width, 2*height), interpolation = cv2.INTER_CUBIC)
